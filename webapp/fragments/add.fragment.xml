<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<TableSelectDialog id="addfragid" contentWidth="22rem" title="Select Employee" multiSelect="true" rememberSelections="true"
		class="sapUiPopupWithPadding" search="onempsearch" confirm="addConfirm" cancel="_handleValueHelpCancelEmpDialog" resizable="true" draggable="true"
