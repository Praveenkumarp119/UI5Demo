{
	"_version": "1.12.0",
	"sap.app": {
		"id": "service.APP_service",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponentForManifest",
			"version": "0.0.0"
		},
		"dataSources": {
			"ZHRPT_LEAVE_AUDIT_GW_SRV": {
				"uri": "/sap/opu/odata/sap/ZHRPT_LEAVE_AUDIT_GW_SRV/",
				"type": "OData",
				"settings": {
					"localUri": "localService/metadata.xml"
				}
			},
			"ZTR_LOAN_DEBT_PAYEMENT_GW_SRV": {
				"uri": "/sap/opu/odata/sap/ZTR_LOAN_DEBT_PAYEMENT_GW_SRV/",
				"type": "OData",
				"settings": {
					"localUri": "localService/ZTR_LOAN_DEBT_PAYEMENT_GW_SRV/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "service.APP_service.view.Display",
			"type": "XML",
			"async": true,
			"id": "Display"
		},
		"dependencies": {
			"minUI5Version": "1.35.0",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "service.APP_service.i18n.i18n"
				}
			},
			"": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request"
				},
				"dataSource": "ZHRPT_LEAVE_AUDIT_GW_SRV",
				"preload": true
			},
			"addModel": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request"
				},
				"dataSource": "ZTR_LOAN_DEBT_PAYEMENT_GW_SRV",
				"preload": true
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "service.APP_service.view",
				"controlAggregation": "pages",
				"controlId": "app",
				"clearControlAggregation": false
			},
			"routes": [
				{
					"name": "RouteDisplay",
					"pattern": "RouteDisplay",
					"target": [
						"TargetDisplay"
					]
				}
			],
			"targets": {
				"TargetDisplay": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewId": "Display",
					"viewName": "Display"
				}
			}
		}
	},
	"sap.platform.abap": {
		"uri": "/sap/bc/ui5_ui5/sap/zapp_service/webapp",
		"_version": "1.1.0"
	}
}